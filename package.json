{
  "name": "@syntest/javascript",
  "version": "0.0.1",
  "description": "SynTest JavaScript is a tool for automatically generating test cases for the JavaScript language",
  "keywords": [
    "syntest",
    "automatic-test-generation"
  ],
  "homepage": "https://www.syntest.org",
  "bugs": {
    "url": "https://github.com/syntest-framework/syntest-javascript/issues"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Annibale Panichella",
    "Mitchell Olsthoorn",
    "Dimitri Stallenberg"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "/src/**/*.js",
    "/api.js",
    "/NOTICE"
  ],
  "bin": {
    "syntest-javascript": "dist/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syntest-framework/syntest-javascript.git"
  },
  "scripts": {
    "build:framework": "npm --prefix ../syntest-framework run build",
    "build": "tsc --build",
    "build:compile": "tsc --build",
    "build:watch": "tsc --build --watch",
    "clean": "rm -rf .nyc_output dist node_modules",
    "clean:dist": "rm -rf dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "npm run build",
    "run": "node --inspect ./dist/bin.js",
    "run-ts": "npm run build:framework; node -r ts-node/register --max-old-space-size=8000 ./src/bin.ts",
    "run-ts:crash": "npm run build:framework; NVM_DIR=/local/scratch/.nvm ts-node ./src/crash-reproduction/index.ts",
    "test": "mocha",
    "test:coverage": "nyc mocha",
    "test:watch": "mocha --watch"
  },
  "devDependencies": {
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/chai": "^4.3.0",
    "@types/figlet": "^1.5.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@types/sinon": "^10.0.6",
    "@types/store": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "complexity-report-html": "^0.0.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.4.1",
    "sinon": "^12.0.1",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=10.24.0"
  },
  "dependencies": {
    "@babel/core": "7.16.5",
    "@babel/plugin-transform-literals": "^7.18.9",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/plugin-transform-strict-mode": "^7.18.6",
    "@babel/plugin-transform-typescript": "^7.19.3",
    "@babel/preset-env": "7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "7.18.9",
    "@istanbuljs/schema": "0.1.3",
    "@syntest/framework": "file:../syntest-framework",
    "@types/babel-core": "^6.25.7",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "esm": "^3.2.25",
    "figlet": "^1.5.2",
    "fs-extra": "^10.0.0",
    "istanbul-lib-instrument": "^5.1.0",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "original-require": "^1.0.1",
    "regenerator-runtime": "^0.13.9",
    "rxjs": "^7.5.6",
    "store": "^2.0.12",
    "ts-babel-node": "^1.1.1",
    "ts-node": "10.8.1"
  }
}
